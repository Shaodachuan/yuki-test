<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="a.xml" />

    <bean id="car" class="com.yuki.demo5.lookup.Car" scope="prototype">
        <property name="brand">
            <value><![CDATA[红旗&4CA72]]></value>
        </property>
    </bean>

    <bean id="boss" class="com.yuki.demo5.lookup.Boss"
          p:car-ref="car" />

    <bean id="bossMagic" class="com.yuki.demo5.lookup.BossMagic"/>

    <bean id="lookupBoss" class="com.yuki.demo5.lookup.LookupBoss">
        <lookup-method name="getCar" bean="car" />
    </bean>
 </beans>